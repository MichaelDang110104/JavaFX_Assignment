<BorderPane prefHeight="408.0" prefWidth="800.0" stylesheets="@CarRentalManagement.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.AdminReviewController">
    <top>
        <AnchorPane prefHeight="200.0" prefWidth="800.0" BorderPane.alignment="CENTER">
            <TableView fx:id="rentalTable" layoutY="64.0" onMouseClicked="#getData" prefHeight="346.0" prefWidth="597.0">
                <columns>
                    <TableColumn fx:id="carId_tbl" prefWidth="94.6666589975357" text="Car ID" />
                    <TableColumn fx:id="customerId_tbl" prefWidth="134.0000228881836" text="Customer ID" />
                    <TableColumn fx:id="comment_tbl" prefWidth="158.00001525878906" text="Comment" />
                    <TableColumn fx:id="reviewStar_tbl" minWidth="0.0" prefWidth="210.66659545898438" text="Review Star" />
                </columns>
            </TableView>
            <Label layoutX="335.0" layoutY="21.0" prefHeight="32.0" prefWidth="234.0" text="Review Management">
                <font>
                    <Font size="22.0" />
                </font>
            </Label>
            <Button layoutX="635.0" layoutY="223.0" mnemonicParsing="false" onAction="#deleteReview" text="DELETE REVIEW" />
            <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="600.0" layoutY="267.0" pickOnBounds="true" preserveRatio="true">
                <image>
                    <Image url="@../images/sports-car-silhouette-dealer-logo-vector-39355097.jpg" />
                </image>
            </ImageView>
         <Hyperlink layoutX="629.0" layoutY="185.0" onAction="#RedirectDashboard" text="Back to dash board?" />
